<?js
var data = obj;
var self = this;
?>
<dt>
    <h4 id="<?js= id ?>" data-name="<?js= name ?>">
      <?js= data.attribs + name + (data.signature ? data.signature : '') ?>
      <?js if (data.deprecated) { ?><span class="label label-warning">deprecated</span><?js } ?>
    </h4>
    <?js if (data.summary) { ?>
    <p class="summary"><?js= summary ?></p>
    <?js } ?>
</dt>
<dd>
    <?js if (data && data.description) { ?>
    <div class="description members-description">
        <?js= data.description ?>
    </div>
    <?js } ?>

    <?js if (data && data.type && data.type.names) {?>
        <h5>Type:</h5>
        <ul>
            <li>
                <?js= self.partial('type.tmpl', data.type.names) ?>
            </li>
        </ul>
    <?js } ?>

    <?js= this.partial('details.tmpl', data) ?>

    <?js if (data.examples && examples.length) { ?>
        <h5>Example<?js= examples.length > 1? 's':'' ?></h5>
        <?js= this.partial('examples.tmpl', examples) ?>
    <?js } ?>
</dd>
